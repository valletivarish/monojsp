	<%
		ArrayList<Employee> employees=new ArrayList<Employee>();	
		employees.add(new Employee("Varish",1,10000.0));
		employees.add(new Employee("Shivamani",2,10000.0));
		employees.add(new Employee("Ajay",3,10000.0));
		pageContext.setAttribute("myEmployees", employees);
	%>
	<table class="table table-striped">
		<tr>
			<td>Name</td>
			<td>ID</td>
			<td>Salary</td>
		</tr>
	<c:forEach var="emp" items="${myEmployees}">
		<tr>${emp.name}</tr>
		<tr>${emp.id}</tr>
		<tr>${emp.salary}</tr>
	</c:forEach>
